        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <config-file parent="/*" target="AndroidManifest.xml">
        <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
        <uses-permission android:name="android.permission.CAMERA" />
        <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
        <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
        <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
        <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
        <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
        <uses-feature android:name="android.hardware.location.gps" />
    </config-file>
    <platform name="ios">
        <gap:config-file parent="NSLocationUsageDescription" platform="ios">
            <string>Your location is collected in order to inform you of the distance of each location as well as to collect some usage data.</string>
        </gap:config-file>
        <gap:config-file parent="NSLocationWhenInUseUsageDescription" platform="ios">
            <string>Your location is collected in order to inform you of the distance of each location as well as to collect some usage data.</string>
        </gap:config-file>
        <gap:config-file parent="NSCameraUsageDescription" platform="ios">
            <string>The camera is used to capture QR codes to collect points and redeem rewards.</string>
        </gap:config-file>
        <gap:config-file parent="NSPhotoLibraryUsageDescription" platform="ios">
            <string>The camera is used to capture QR codes to collect points and redeem rewards.</string>
        </gap:config-file>
        <gap:config-file parent="NSBluetoothPeripheralUsageDescription" platform="ios">
            <string>This is not actually used, but bundled up in one permission.</string>
        </gap:config-file>
        <gap:config-file parent="NSMicrophoneUsageDescription" platform="ios">
            <string>This is not actually used, but bundled up in one permission.</string>
        </gap:config-file>
    </platform>
